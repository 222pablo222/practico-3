#include <stdio.h>

int main() {
    int n;
    long long f0 = 1, f1 = 1, fn;  
    printf("Ingrese el valor de n (posición del número Fibonacci): ");
    scanf("%d", &n);

    if (n < 0) {
        printf("n debe ser un número no negativo.\n");
        return 0;
    }

    if (n == 0) {
        printf("F0 = 1\n");
    }
    else if (n == 1) {
        printf("F1 = 1\n");
    }
    else {
        for (int i = 2; i <= n; i++) {
            fn = f0 + f1;  
            f0 = f1;       
            f1 = fn;
        }
        printf("F%d = %lld\n", n, f1);
    }

    return 0;
}
