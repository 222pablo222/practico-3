#include <stdio.h>

int main() {
    int n;
    long long f0 = 1, f1 = 1, fn;

    printf("Ingrese el valor de n (posición del número Fibonacci, empezando en 0): ");
    scanf("%d", &n);

    if (n < 0) {
        printf("n debe ser un número no negativo.\n");
        return 0;
    }

    printf("Secuencia de Fibonacci hasta F%d:\n", n);

    if (n == 0) {
        printf("F0 = 1\n");
        return 0;
    }

    printf("F0 = 1\n");
    printf("F1 = 1\n");

    for (int i = 2; i <= n; i++) {
        fn = f0 + f1;   
        printf("F%d = %lld\n", i, fn);
        f0 = f1;        
    }

    printf("\nEl %d-ésimo número de Fibonacci es: %lld\n", n, fn);

    return 0;
}
