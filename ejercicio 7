#include <stdio.h>

int main() {
    int N, i;
    unsigned long long factorial = 1;  

    printf("Ingrese un número entero: ");
    scanf("%d", &N);

    if (N < 0) {
        printf("El factorial no está definido para números negativos.\n");
        return 0;
    }

    for (i = 1; i <= N; i++) {
        factorial *= i;
    }

    printf("El factorial de %d es: %llu\n", N, factorial);

    return 0;
}
