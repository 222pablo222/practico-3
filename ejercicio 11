#include <stdio.h>

int main() {
    int a, b, resto;

    printf("Ingrese el primer número: ");
    scanf("%d", &a);
    printf("Ingrese el segundo número: ");
    scanf("%d", &b);

    if (a < 0) a = -a;
    if (b < 0) b = -b;

    while (b != 0) {
        resto = a % b;
        a = b;
        b = resto;
    }

    printf("El MCD es: %d\n", a);
    return 0;
}
